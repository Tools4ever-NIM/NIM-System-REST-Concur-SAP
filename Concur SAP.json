    "rest": {
        "config": {
            "baseUrl": "{geolocation}",
            "get": {
                "maxPageCount": 5,
                "pagination": {
                    "mode": "url",
                    "replace_in_url": true,
                    "last_page": {
                        "type": "token",
                        "path": "nextCursor"
                    },
                    "query_params": [
                        {
                            "name": "cursor",
                            "value": {
                                "type": "response_field",
                                "value": "nextCursor"
                            }
                        }
                    ]
                }
            },
            "authentication": "password_grant",
            "call_handling": "generic",
            "maxCallsPerMinute": 100,
            "retry": [
                {
                    "type": "statusCode",
                    "statusCode": 503
                },
                {
                    "type": "connectionError"
                }
            ],
            "test_connection": {
                "url": "/profile/identity/v4.1/Users?count=1"
            },
            "accept": "application/json"
        },
        "authOptions": {
            "type": "password_grant",
            "authUrl": "{geolocation}/oauth2/v0/token",
            "extraTokenRequestData": {
                "credtype": "authtoken"
            },
            "extraParameters": {
                "geolocation": "geolocation"
            }
        }
    },
    "connection": {
        "items": [
            {
                "order": 1,
                "name": "user_name",
                "type": "textbox",
                "label": "Company UUID",
                "value": "",
                "required": true,
                "tooltip": "Use the Concur SAP Company Request Token Self-Service Tool to get this information. A refresh token has a six month lifetime"
            },
            {
                "order": 2,
                "name": "password",
                "type": "textbox",
                "password": true,
                "label": "Company Request Token",
                "value": "",
                "required": true,
                "tooltip": "Use the Concur SAP Company Request Token Self-Service Tool to get this information. A refresh token has a six month lifetime"
            },
            {
                "name": "client_id",
                "order": 3,
                "required": true,
                "tooltip": "Use the Concur SAP OAuth 2.0 Application Management Tool to generate Client IDs (App IDs) and Client Secrets without contacting SAP Concur support"
            },
            {
                "name": "client_secret",
                "order": 4,
                "required": true,
                "tooltip": "Use the Concur SAP OAuth 2.0 Application Management Tool to generate Client IDs (App IDs) and Client Secrets without contacting SAP Concur support"
            },
            {
                "order": 300,
                "name": "geolocation",
                "type": "textbox",
                "label": "Geo Location:",
                "value": "https://us.api.concursolutions.com",
                "hidden": "!item"
            }
        ]
    }
